{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "cache": {
        "key": "gradle-cache-local",
        "paths": [
          "android/.gradle",
          "~/.gradle/caches"
        ]
      }
    },
    "production": {
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}
